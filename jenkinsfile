node {
   stage('scm checout'){
   git credentialsId: 'e135db2b-4595-4e3e-aaae-d2cc3b4be499', url: 'https://github.com/nareshjp7/warproject.git'
}
   stage('mvn pak'){
       def MVNHOME=tool name: 'mvn', type: 'maven'
       def MVNCMD="${MVNHOME}/bin/mvn"
       sh"${MVNCMD} clean package"
   }
   stage('build image'){
       sh'docker build -t devnj/my-app1:v2.0 .'
   }    
   stage('tag image'){
       sh'docker tag devnj/my-app1:v2.0 devnj/my-app1:$BUILD_ID .'    
       
   }
   stage('push doc image'){
      withCredentials([string(credentialsId: 'dockerhub', variable: 'dockerHubPwd')]) {
          sh "docker login -u devnj -p ${dockerHubPwd}"
   }
   sh 'docker push devnj/my-app1:v2.0'
   }
   stage('run doc on dev env'){
       def dockerRun='docker run -d -p 8080:8087 --name doc10 devnj/my-app1:v2.0'
       sshagent(['dev-server']) {
         sh "ssh -o stricthostkeychecking=no ec2-user@3.86.111.166 ${dockerRun}"
          
   }
       
   }
    
   }
